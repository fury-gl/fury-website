    <title type="text">Google Summer of Code</title>
    <id>https://fury.gl/posts/2020/2020-01-05-gsoc.html</id>
    <updated>2020-01-05T00:00:00Z</updated>
    <published>2020-01-05T00:00:00Z</published>
